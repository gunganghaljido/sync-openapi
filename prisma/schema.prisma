generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Facility {
  businessId    String @id
  owner         String
  cityCode      String
  cityName      String
  localCode     String
  localName     String
  name          String
  phone         String
  address       String
  detailAddress String
}
